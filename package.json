{
  "name": "nestjs-new-template",
  "version": "1.0.1",
  "description": "A NestJS boilerplate for enterprises with modular folder structure",
  "author": "Nguyen Quang Tu",
  "private": true,
  "license": "MIT",
  "scripts": {
    "command": "ts-node -r tsconfig-paths/register ./node_modules/plop/bin/plop.js",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --dataSource ./src/modules/base/database/database.providers.ts",
    "typeorm-seeding": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js",
    "seed:run": "yarn typeorm-seeding seed -n index.ts -r ./src/core/utils/seeder",
    "schema:drop": "yarn typeorm schema:drop",
    "prepare-husky": "husky install"
  },
  "dependencies": {
    "@eshopping/helper-package": "^1.3.0",
    "@faker-js/faker": "^7.5.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.1",
    "@sentry/node": "^7.21.1",
    "@sentry/tracing": "^7.21.1",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.0.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.1",
    "cloudinary": "^1.32.0",
    "dotenv": "^10.0.0",
    "lint-staged": "^13.0.3",
    "logform": "^2.2.0",
    "moment": "^2.29.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "prettier": "^2.7.1",
    "redis": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.3.7",
    "typeorm-seeding": "^1.6.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-logstash": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
